<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    
    <record id="account_payment_view_tree" model="ir.ui.view">
        <field name="name">account.payment.view.tree.inherited.last.payment</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="categoria_cliente"/>
                <field name="cliente_termino_pago_store"/>
            </xpath>
        </field>
    </record>

    <record id="account_payment_view_form" model="ir.ui.view">
        <field name="name">account.payment.view.form.inherited.categoriacliente</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="categoria_cliente"/>
                <field name="cliente_termino_pago_store"/>
            </xpath>
        </field>
    </record>

    <record id="account_payment_view_form_tipocambio" model="ir.ui.view">
        <field name="name">account.payment.view.form.inherited.tipocambio</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position="after">
                <field name="tipocambio_pago_store"/>
            </xpath>
        </field>
    </record>
    
</odoo>